{
    "name": "icecave/lace",
    "description": "A DSN parser for Doctrine databases and cache drivers.",
    "keywords": [
        "dsn",
        "doctrine",
        "parser",
        "postgres",
        "mysql",
        "sqlite",
        "redis",
        "heroku"
    ],
    "homepage": "https://github.com/IcecaveStudios/lace",
    "license": "MIT",
    "authors": [
        {
            "name": "James Harris",
            "email": "james.harris@icecave.com.au",
            "homepage": "https://github.com/jmalloc"
        }
    ],
    "require": {
        "php": ">=5.5"
    },
    "require-dev": {
        "doctrine/cache": "~1",
        "icecave/archer": "~1",
        "icecave/isolator": "~2"
    },
    "suggest": {
        "doctrine/cache": "Allows Lace to construct and configure cache providers from a DSN."
    },
    "autoload": {
        "psr-4": {
            "Icecave\\Lace\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.2.x-dev"
        }
    }
}
